<script src="https://cdn.bootcss.com/jquery/3.2.1/jquery.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $.ajax({
            type:'GET',
            url:"/api/api/seminar/2",
            dataType:"json",
            success:function (result){
                $("#descrip").append('<span class="title">' +"result.Data.Seminar.courseName”+'</span>'+
                '<span class="title2">' +"result.Data.name"+'</span>');
            }
        });
    })
</script>

